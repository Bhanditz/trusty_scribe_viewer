<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>{% block title %}(No title){% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1">
	<style type="text/css">
a:link {
    color : #338;
    text-decoration : none;
}
a:visited {
    color : #338;
    text-decoration : none;
}
a:hover {
    text-decoration : underline;
}
body {
    font-family : sans-serif;
    margin : 40px auto;
    max-width : 650px;
    line-height : 1.6;
    font-size : 16px;
    color : #333;
    padding : 10px
}
h1,h2,h3 {
    line-height :1.2
}
img {
    max-height : 300px;
}
pre {
    font-size : 12px;
    display: block;
    padding: 0.5em 1em 0.5em 1em;
    background: #eee;
    overflow: auto;
    max-height: 300px;
    margin : 0;
}
table {
    border-spacing: 0;
}
td {
    vertical-align : top;
    padding : 0px 30px 0px 0px;
}

.link_and_content {
    margin : 0px 20px 0px 20px;
}
	</style>
    </head>
    <body>
      {% block content %}(No content){% endblock %}
    </body>
</html>
