{% extends "base.djhtml" %}

{% block title %}
  Lab Notebook
{% endblock %}

{% block content %}
  <h1>Lab Notebook for {{ repo_name }}</h1>
  {% for commit in commit_list %}
    <h3><a href="/commit/{{ commit.commit_id }}/">{{ commit.title }}</a></h3>
    <p>{{ commit.content }}</p>
  {% endfor %}
  <a href="/lab_notebook/">Show all</a>
{% endblock %}
